{
  "name": "html-text-parser",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/react.cjs.js",
  "module": "dist/react.esm.js",
  "exports": {
    ".": {
      "import": "./dist/react.esm.js",
      "require": "./dist/react.cjs.js"
    },
    "./react": {
      "import": "./dist/react.esm.js",
      "require": "./dist/react.cjs.js"
    },
    "./vue": {
      "import": "./dist/vue.esm.js",
      "require": "./dist/vue.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "@types/react": "*",
    "react": "*",
    "vue": "*"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/jest": "^30.0.0",
    "jest": "^30.0.5",
    "rollup": "^4.46.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2"
  },
  "dependencies": {},
  "scripts": {
    "test": "jest",
    "build": "rollup -c",
    "clean": "rm -rf dist",
    "prepare": "yarn build"
  },
  "optionalDependencies": {
    "@types/react": "*",
    "react": "*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darotar/html-text-parser.git"
  },
  "author": "darotar fabledva@mail.ru",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/darotar/html-text-parser/issues"
  },
  "homepage": "https://github.com/darotar/html-text-parser#readme"
}
